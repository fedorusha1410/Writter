<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:local="clr-namespace:Writter">
    <Style x:Key="Latter_Name_user" TargetType="TextBlock">
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="FontFamily" Value="/Writter;component/Resourses/#Lato"></Setter>
        <Setter Property="Foreground" Value="#FFE91E63"></Setter>
        <Setter Property="FontSize" Value="65"></Setter>
    </Style>
</ResourceDictionary>